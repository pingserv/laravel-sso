{
    "name": "pingserv/laravel-sso",
    "description": "Simple PHP SSO integration for Laravel",
    "keywords": ["laravel", "sso", "authentication", "login"],
    "homepage": "https://github.com/pingserv/laravel-sso",
    "license": "MIT",
    "authors": [
      {
        "name": "Kiss Ervin",
        "email": "pingserv@gmail.com",
        "homepage": "https://github.com/pingserv",
        "role": "Developer"
      }
    ],
    "require": {
        "php": ">=7.1.3",
        "guzzlehttp/guzzle": "^6.3",
        "laravel/framework": "^5.5",
        "zefy/php-simple-sso": "^1.0"
    },
    "autoload": {
      "psr-4": {
        "Pingserv\\LaravelSSO\\": "src/"
      }
    },
    "extra": {
      "laravel": {
        "providers": [
            "Pingserv\\LaravelSSO\\SSOServiceProvider"
        ]
      }
    }
}